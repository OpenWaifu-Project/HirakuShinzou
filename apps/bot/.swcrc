{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": false,
    "module": {
        "type": "commonjs",
        "strictMode": true,
        "noInterop": false
    },
    "jsc": {
        "externalHelpers": false,
        "target": "es2022",
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "keepClassNames": true,
        "baseUrl": "."
    }
}